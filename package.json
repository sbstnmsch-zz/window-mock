    "babel-runtime": "6.26.0",
    "babel-tape-runner": "2.0.1",
    "faucet": "0.0.1",
    "grunt": "1.0.2",
    "grunt-cli": "1.2.0",
    "grunt-eslint": "20.1.0",
    "grunt-shell": "2.1.0",
    "load-grunt-config": "0.19.2",
    "node-libs-browser": "2.1.0",
    "tape": "4.9.0",
    "time-grunt": "1.4.0"
  },
  "license": "MIT",
  "keywords": [
    "mock",
    "window",
    "node"
  ],

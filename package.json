    "babel-tape-runner": "1.3.0",
    "faucet": "0.0.1",
    "grunt": "0.4.5",
    "grunt-cli": "0.1.13",
    "grunt-eslint": "17.3.1",
    "grunt-shell": "1.1.2",
    "load-grunt-config": "0.19.0",
    "node-libs-browser": "0.5.3",
    "tape": "4.0.1",
    "time-grunt": "1.1.0"
  },
